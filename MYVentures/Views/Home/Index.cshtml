@{
    ViewBag.Title = "Home";
    ViewBag.UploadLocation = MYVentures.Helpers.CustomActions.GetUploadLocation();
    ViewBag.MainClass = "main_landing";
}
   <!-- ========== Hero Cover ========== -->
<div class="kenburn-hero animated-hero" image-data="@Html.Raw(ViewBag.imgList)">
    <div class="kenburn-slider bg-overlay">

        <!-- Heading -->
        <div class="hero-wrapper  hero-content-wrapper">

            <!-- Hero Content -->
            <div class="hero-content ">

                <h1 class="cd-headline zoom hero-lead">
                    <span class="cd-words-wrapper">
                        @{ var vflag = true; }
                        @foreach(var ms in ViewBag.HeaderList)
                        {
                            <b class='@(vflag ? "is-visible" : "" )'>
                                @ms.head
                                <p>@Html.Raw(ms.text)</p>
                                @{ vflag = false; }
                            </b>
                        }
                    </span>
                </h1>

            </div>

        </div><!-- / .hero-wrapper -->
        <!-- Ken Burns Slider -->
        <canvas class="kenburns">
            <p>Your browser doesn't support canvas!</p>
        </canvas>
    </div><!-- / .kenburn-slider -->
</div><!-- / #home .kenbrun-hero-->

<!-- ========== About ========== -->
<section id="welcome" class="container">
    <div class="row">
        <div class="ws-l"></div>

        <header class="sec-heading">
            <h2>Welcome to MY Ventures</h2>
            <span class="subheading">@ViewBag.Whead.MetaText</span>
        /</header>
        <div class="col-md-offset-2 col-md-8 text-center ws-m">@Html.Raw(ViewBag.Wtext.MetaText)</div>
        <div class="col-md-12">
            <img class="img-responsive wow fadeIn" data-wow-duration="2s" alt="MYVentures Welcome Image"
                 src='@ViewBag.UploadLocation@(ViewBag.Wimg.MetaText == "" ? "image_placeholder.jpg" : ViewBag.Wimg.MetaText)'>
        </div>
    </div><!-- / .row -->
</section><!-- / .section -->
