<!-- ========== Contact ========== -->
<section id="contact" class="section contact-1">

    <header class="sec-heading">
        <h2>Countact Us</h2>
        <span class="subheading">We love to hear from you</span>
    </header>

    <div class="contact-wrapper">
        <!-- Map -->
        <div id="map-canvas" class="gmap map-boxed"></div>
        <!-- Show Info Button -->
        <div class="show-info-link">
            <a href="#" class="show-info">
                <i class="fa fa-info"></i>
                <h6>Show info</h6>
            </a>
        </div>
        <div class="container">
            <div class="row ws-m">
                <!-- Address Info -->
                <div class="col-xs-offset-1 col-xs-11 col-md-offset-2 col-md-6 contact-info-wrapper">
                    <address>
                        <div class="row">
                            <!-- Phone -->
                            <div class="col-sm-6 address-group">
                                <span>Phone</span>
                                <a href="#">@Html.Raw(ViewBag.phone.MetaText)</a>
                            </div>
                            <!-- Address -->
                            <div class="col-sm-5 address-group">
                                <span>Address</span>
                                <p>@Html.Raw(ViewBag.location.MetaText)</p>
                            </div>
                        </div><!-- / .row -->
                        <div class="row">
                            <!-- Email -->
                            <div class="col-sm-6 address-group">
                                <span>Email</span>
                                <a href="mailto:@ViewBag.email.MetaText">@ViewBag.email.MetaText</a>
                            </div>
                            <!-- Hours -->
                            <div class="col-sm-5 address-group">
                                <span>Open Hours</span>
                                <p>@ViewBag.open.MetaText</p>
                            </div>
                        </div><!-- / .row -->
                        <!-- Show Map Button -->
                        <div class="row show-map-link">
                            <a href="#" class="show-map"><span class="icon-map-pin"></span>Show on map</a>
                        </div><!-- / .row -->
                    </address>
                </div><!-- / .contact-info-wrapper -->
            </div><!-- / .row -->
            <!-- Contact Form -->
            <div class="row">
                <form action="/Home/SendMessage" method="POST" class="form-ajax">
                    <div class="col-md-offset-2 col-md-4 wow fadeInUp" data-wow-duration="1s">
                        <!-- Name -->
                        <div class="form-group">
                            <input type="text" name="model.name" id="name-contact-1" class="form-control validate-locally" placeholder="Enter your name">
                            <label for="name-contact-1">Name</label>
                            <span class="pull-right alert-error"></span>
                        </div>
                        <!-- Email -->
                        <div class="form-group">
                            <input type="email" name="model.email" id="email-contact-1" class="form-control validate-locally" placeholder="Enter your email">
                            <label for="email-contact-1">Email</label>
                            <span class="pull-right alert-error"></span>
                        </div>
                        <div class="form-group">
                            <input type="text" name="model.phone" id="email-contact-1" class="form-control validate-locally" placeholder="Enter your phone number">
                            <label for="phone-contact-1">Phone Number</label>
                            <span class="pull-right alert-error"></span>
                        </div>
                    </div>
                    <div class="col-md-4 wow fadeInUp" data-wow-duration="1s">
                        <!-- Message -->
                        <div class="form-group">
                            <textarea name="model.message" id="message-contact-1" class="form-control" rows="8" placeholder="Your Message"></textarea>
                            <label for="message-contact-1">Message</label>
                        </div>
                        <div>
                            <input type="submit" class="btn pull-right" value="Send Message">
                        </div>
                        <!-- Ajax Message -->
                        <div class="ajax-message col-md-12 no-gap"></div>
                    </div><!-- / .col-md-4 -->
                </form>
            </div><!-- / .row -->
        </div><!-- / .container -->
    </div><!-- / .contact-wrapper -->
</section><!-- / .contact-1 -->

